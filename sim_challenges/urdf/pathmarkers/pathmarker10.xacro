<?xml version="1.0"?>
<robot name="path_marker10" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!-- Introducir meshes -->
<xacro:property name="visual_mesh_file" value="file://$(find sim_challenges)/models/meshes/pathmarkers/pathmarker10.dae" />

<!-- <origin xyz="0.085 (0 -0.085) 0 0" rpy="0.0 0.0 0.0075"/> -->

<link name="path_marker_base10">
    <visual>
        <origin xyz="0 0 -4" rpy="0.0 0.0 1.57"/>
        <geometry>
            <mesh filename="${visual_mesh_file}" scale="1 1 1" />      
        </geometry>
    </visual>

        <!-- Se deben cambiar los parametros reales-->
    <collision>
      <origin xyz="0 0 -4" rpy="0.0 0.0 1.57"/>
      <geometry>
        <mesh filename="${visual_mesh_file}" scale="1 1 1" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 -4" rpy="0.0 0.0 1.57"/>
      <mass value="1"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
</link>
<gazebo reference="path_marker_base10">
  <selfCollide>0</selfCollide>
  <turnGravityOff>1</turnGravityOff>
</gazebo>


</robot> 